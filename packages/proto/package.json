{
  "name": "@merjs/proto",
  "version": "0.0.10",
  "files": [
    "src/",
    "cosmos/",
    "ethermint/",
    "google/",
    "ibc/",
    "merlion/",
    "tendermint/",
    "proofs.d.ts",
    "proofs.d.ts.map",
    "proofs.js",
    "proofs.js.map",
    "*.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "gen": "protoc --ts_out src --ts_opt long_type_string --proto_path $(PWD)/proto $(find $(PWD)/proto -iname '*.proto')",
    "build": "node scripts/rm.mjs && rimraf ./dist && tsc -b && node scripts/cp.mjs",
    "postpublish": "node scripts/rm.mjs"
  },
  "dependencies": {
    "@protobuf-ts/runtime": "^2.5.0",
    "@protobuf-ts/runtime-rpc": "^2.5.0"
  },
  "devDependencies": {
    "@protobuf-ts/plugin": "^2.5.0",
    "@protobuf-ts/protoc": "^2.5.0"
  }
}
